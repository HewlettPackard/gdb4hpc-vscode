{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"outDir": "dist",
		"lib": ["ES2020"],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"noImplicitAny": false,
		"removeComments": false,
		"noUnusedLocals": true,
		"noImplicitThis": true,
		"inlineSourceMap": false,
		"preserveConstEnums": true,
		"strictNullChecks": true,
		"noUnusedParameters": false,
		"allowJs": true,
		"strictPropertyInitialization": false
	},
}
